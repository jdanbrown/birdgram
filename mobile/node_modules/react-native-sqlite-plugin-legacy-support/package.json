{
  "name": "react-native-sqlite-plugin-legacy-support",
  "title": "React Native SQLite Plugin legacy support",
  "version": "0.0.1-dev",
  "description": "Native interface to SQLite for React Native - react-native-sqlite-plugin-legacy-support version",
  "main": "index.js",
  "cordova": {
    "id": "react-native-sqlite-plugin-support-dev",
    "platforms": [
      "android",
      "ios",
      "osx",
      "windows"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brodybits/react-native-sqlite-plugin-legacy-support.git",
    "baseUrl": "https://github.com/brodybits/react-native-sqlite-plugin-legacy-support"
  },
  "keywords": [
    "react-native",
    "sqlite",
    "ecosystem:cordova",
    "cordova-android",
    "cordova-ios",
    "cordova-osx",
    "cordova-windows"
  ],
  "author": {
    "name": "various contributors",
    "email": "sales@xpbrew.consulting"
  },
  "bugs": {
    "url": "https://github.com/brodybits/react-native-sqlite-plugin-legacy-support/issues"
  },
  "homepage": "https://github.com/brodybits/react-native-sqlite-plugin-legacy-support",
  "license": "MIT",
  "licenseFilename": "LICENSE",
  "readmeFilename": "README.md",
  "scripts": {
    "clean-spec": "rm -rf spec/[mnp]* && git cl spec/config.xml && git st",
    "prepare-js": "coffee -p SQLitePlugin.coffee.md > www/SQLitePlugin.js",
    "prepare-spec": "node scripts/prepareSpec.js",
    "spec-help": "echo 'do `npm run prepare-spec` then build & run test app in `spec`'",
    "test": "echo 'npm test is not supported' && npm run spec-help && exit 1"
  },
  "peerDependencies": {
    "react": "^16.8.3",
    "react-native": "^0.59.0"
  },
  "dependencies": {
    "cordova-sqlite-storage-dependencies": "2.0.0"
  },
  "devDependencies": {
    "coffeescript": "1",
    "cross-spawn": "^6.0.5",
    "fs-extra": "^7.0.1",
    "react": "^16.8.3",
    "react-native": "^0.59.0"
  }
}
